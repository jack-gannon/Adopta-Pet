{
  "extends": ["eslint:recommended", "airbnb"],
  "plugins": ["prettier", "flowtype"],
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "quotes": 0,
    "no-console": 0,
    "react-hooks/rules-of-hooks": "error"
  },
  "env": {
    "es6": true,
    "jest": true,
    "browser": true,
    "node": true
  }
}
